{
  "type": "object",
  "title": "Extractor configuration",
  "required": [
    "file_name",
    "incremental",
    "dates",
    "current_as_today",
    "currency"
  ],
  "properties": {
    "file_name": {
      "type": "string",
      "title": "Table name",
      "description": "Your data will be extracted to in.c-cnb-extractor.{your table name}",
      "minLength": 3,
      "default": "xxx",
      "propertyOrder": 100
    },
    "incremental": {
      "type": "boolean",
      "format": "checkbox",
      "title": "Incremental load",
      "description": "By checking this box newly extracted data will be loaded incrementally to the Storage using the 'date' and 'code' primary key. Please note that if you go later from incremental load to non-incremental your previously downloaded data will be overwritten!",
      "default": true,
      "propertyOrder": 200
    },
    "dates": {
      "type": "string",
      "title": "Range of dates",
      "enum": [
        "Current day (currently declared rates)",
        "Current day and yesterday",
        "Week",
        "Custom date range"
      ],
      "default": "Current day and yesterday",
      "propertyOrder": 300
    },
    "dependent_date_from": {
      "title": "Date from",
      "type": "string",
      "format": "date",
      "options": {
        "flatpickr": {},
        "dependencies": {
          "dates": "Custom date range"
        }
      },
      "propertyOrder": 500
    },
    "dependent_date_to": {
      "title": "Date to",
      "type": "string",
      "format": "date",
      "options": {
        "flatpickr": {},
        "dependencies": {
          "dates": "Custom date range"
        }
      },
      "propertyOrder": 550
    },
    "current_as_today": {
      "type": "boolean",
      "format": "checkbox",
      "title": "Current rates as today's rates",
      "description": "By checking this box actual exchange rates will be considered as exchange rates for the current day (Exchange rates for current day are declared every working day after 2.30 p.m. CET). Please use this option together with incremental load so the current rates can be updated on the next run.",
      "default": true,
      "propertyOrder": 600
    },
    "currency": {
      "title": "Select currency",
      "type": "string",
      "enum": [
        "All",
        "Selected currencies"
      ],
      "default": "All",
      "propertyOrder": 700
    },
    "dependent_AUD": {
      "title": "Austrálie (dolar)",
