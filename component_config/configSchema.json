{
  "type": "object",
  "title": "Extractor configuration",
  "required": [
    "file_name",
    "incremental",
    "current_as_today",
    "dates"
  ],
  "properties": {
    "file_name": {
      "type": "string",
      "title": "Table name",
      "description": "Your table will be located under in.c-cnb-extractor.xxx",
      "minLength": 3,
      "default": "xxx",
      "propertyOrder": 50
    },
    "incremental": {
      "type": "boolean",
      "format": "checkbox",
      "title": "Incremental load",
      "description": "By checking this box new extracted data will be appended to the output table based on 'date' and 'code' primary key",
      "default": false,
      "propertyOrder": 100
    },
    "current_as_today": {
      "type": "boolean",
      "format": "checkbox",
      "title": "Current rates as today's rates",
      "description": "Checking this box is recommended",
      "default": true,
      "propertyOrder": 100
    },
    "dates": {
      "type": "string",
      "title": "Dates",
      "enum": [
        "today",
        "today + yesterday",
        "week",
        "custom dates range"
      ],
      "propertyOrder": 200,
      "default": "today + yesterday"
    },
    "dependent_date_from": {
      "title": "Date from",
      "type": "string",
      "format": "date",
      "options": {
        "dependencies": {
          "dates": "custom dates range"
        }
      },
      "propertyOrder": 300
    },
    "dependent_date_to": {
      "title": "Date to",
      "type": "string",
      "format": "date",
      "options": {
        "dependencies": {
          "dates": "custom dates range"
        }
      },
      "propertyOrder": 400
    }
  }
}